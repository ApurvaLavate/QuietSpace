<div id="<%= dom_id place %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Category:</strong>
    <%= place.category %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Placename:</strong>
    <%= place.placename %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Location:</strong>
    <%= place.location %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Timing:</strong>
    <%= place.timing %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Booking:</strong>
    <%= place.booking %>
  </div>
</div>
